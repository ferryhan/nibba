    "1.🎨 [GAME] Green ",
    "2.🎨 [GAME] Red ",
    "3.🎨 [GAME] Yellow ",
    "4.🎨 [GAME] RGB ",  
    "🔙"
  }, nil, "🎨Color SD 410/435 Menu🎨")
  if CLSD == nil then else
  if CLSD[1] == true then CL1() end
  if CLSD[2] == true then CL2() end
  if CLSD[3] == true then CL3() end
  if CLSD[4] == true then CL4() end  
  if CLSD[5] == true then
      WNC()
    end
  end
end
