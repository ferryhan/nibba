  "⏹ HEADSHOT ",
  "🔙"
  }, nil, "⚙ Lobby Menu ⚙")
  if LB == nil then
  else
  if LB[1] == true then
      LBY1()
    end
  if LB[2] == true then
      LBY2()
  end
  if LB[3] == true then
      MBBL()
    end
  if LB[4] == true then
      HEAD()
    end  
  if LB[5] == true then
      HOME()
    end
  end
end
