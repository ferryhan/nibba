function GEM()
  GM = gg.multiChoice({
  "⏹ AIMBOT",
  "⏹ ANTENNA",
  "⏹ BULLET",
  "⏹ CAR",
  "⏹ GUN",
  "⏹ VIEW",
  "⏹ ZOOM",
  "⏹ MAIN",    
  "🔙"
  }, nil, "🎮 Game Menu 🎮")
  if GM == nil then
  else
  if GM[1] == true then
      AIM()
    end
  if GM[2] == true then
      ANNT()
  end
  if GM[3] == true then
      BULT()
    end
  if GM[4] == true then
      CRM()
    end
  if GM[5] == true then
      GUNM()
    end 
  if GM[6] == true then
      VIWM()
    end
  if GM[7] == true then
      ZOMM()
    end  
  if GM[8] == true then
      MAN()
    end       
  if GM[9] == true then
      HOME()
    end
  end
end
